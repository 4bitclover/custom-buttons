<?xml version="1.0" encoding="utf-8"?>







<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="45"
                android:centerColor="#ff00b5"
                android:endColor="#006386"
                android:gradientRadius="45"
                android:startColor="#e95a22"
                android:type="sweep" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="rectangle" >
            <solid android:color="#C9D4FF"/>
            <padding
                android:top="10dp"
                android:bottom="10dp"
                android:left="25dp"
                android:right="25dp"/>
        </shape>
    </item>
</layer-list>